<b:comment>
Assets
================================================= </b:comment>

<b:defaultmarkups>
  <b:defaultmarkup type='Common'>
    <b:includable id='assets.requiredCss'>
      <b:comment>=== Bootstrap ===</b:comment>
      <style>
      <!--(bake assets/css/bootstrap/bootstrap.min.css)-->
      </style>
      <b:comment>=== template-blogger ===</b:comment>
      <style>
      <!--(bake assets/css/template-blogger/template-blogger.min.css)-->
      </style>
      <b:comment>=== Font Awesome ===</b:comment>
      <link crossorigin='anonymous' href='{{ blog.cdn.fontAwesome }}' integrity='{{ blog.cdn.fontAwesome_hash }}' rel='stylesheet'/>
    </b:includable>
    <b:includable id='assets.requiredJs'>
      <b:comment>=== jQuery ===</b:comment>
      <script crossorigin='anonymous' integrity='{{ blog.cdn.jquery_hash }}' src='{{ blog.cdn.jquery }}'/>
      <b:comment>=== Popper.js ===</b:comment>
      <script crossorigin='anonymous' integrity='{{ blog.cdn.popper_hash }}' src='{{ blog.cdn.popper }}'/>
      <b:comment>=== Bootstrap ===</b:comment>
      <script crossorigin='anonymous' integrity='{{ blog.cdn.bootstrapJs_hash }}' src='{{ blog.cdn.bootstrapJs }}'/>
    </b:includable>
  </b:defaultmarkup>
</b:defaultmarkups>

